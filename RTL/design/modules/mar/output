S_0x102bd4500 .scope module, "tb" "tb" 2 3;
 .timescale -9 -12;
v0x102bdc110_0 .var "bus", 15 0;
v0x102bdc1b0_0 .var "clk", 0 0;
v0x102bdc250_0 .net "mar_out", 7 0, v0x102bdbe90_0;  1 drivers
v0x102bdc2f0_0 .var "mar_write", 0 0;
v0x102bdc390_0 .var "rst", 0 0;
S_0x102bd4680 .scope module, "dut" "mar" 2 9, 3 1 0, S_0x102bd4500;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "mar_write";
    .port_info 3 /INPUT 16 "bus";
    .port_info 4 /OUTPUT 8 "mar_out";
v0x102bd77d0_0 .net "bus", 15 0, v0x102bdc110_0;  1 drivers
v0x102bd7870_0 .net "clk", 0 0, v0x102bdc1b0_0;  1 drivers
v0x102bdbe90_0 .var "mar_out", 7 0;
v0x102bdbf30_0 .var "mar_out_nxt", 7 0;
v0x102bdbfd0_0 .net "mar_write", 0 0, v0x102bdc2f0_0;  1 drivers
v0x102bdc070_0 .net "rst", 0 0, v0x102bdc390_0;  1 drivers
E_0x102bd8120 .event anyedge, v0x102bdbfd0_0, v0x102bd77d0_0, v0x102bdbe90_0;
E_0x102bd4e20 .event posedge, v0x102bd7870_0;
    .scope S_0x102bd4680;
T_0 ;
    %wait E_0x102bd4e20;
    %load/vec4 v0x102bdc070_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x102bdbe90_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x102bdbf30_0;
    %assign/vec4 v0x102bdbe90_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x102bd4680;
T_1 ;
    %wait E_0x102bd8120;
    %load/vec4 v0x102bdbfd0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x102bd77d0_0;
    %parti/s 8, 0, 2;
    %store/vec4 v0x102bdbf30_0, 0, 8;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x102bdbe90_0;
    %store/vec4 v0x102bdbf30_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x102bd4500;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x102bdc1b0_0;
    %inv;
    %store/vec4 v0x102bdc1b0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x102bd4500;
T_3 ;
    %pushi/vec4 0, 0, 19;
    %split/vec4 16;
    %store/vec4 v0x102bdc110_0, 0, 16;
    %split/vec4 1;
    %store/vec4 v0x102bdc2f0_0, 0, 1;
    %split/vec4 1;
    %store/vec4 v0x102bdc390_0, 0, 1;
    %store/vec4 v0x102bdc1b0_0, 0, 1;
    %vpi_call 2 21 "$dumpfile", "output.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x102bd4500 {0 0 0};
    %delay 100000, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x102bd4500;
T_4 ;
    %delay 12000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x102bdc390_0, 0, 1;
    %delay 8000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x102bdc390_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 74, 0, 16;
    %store/vec4 v0x102bdc110_0, 0, 16;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x102bdc2f0_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x102bdc2f0_0, 0, 1;
    %end;
    .thread T_4;
